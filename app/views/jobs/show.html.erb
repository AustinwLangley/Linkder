<div>
	<h1><%= @job.title %></h1>
	<p>Job Type: <%= @job.job_type %></p>
	<p>Location: <%= @job.location %></p>
	<p>Pay: $<%= @job.salary %></p>
	<p>Skills Needed: <%= @job.skill %></p>
</div>
<div><%= link_to "Home", root_path %></div>
<div><%= link_to "Edit", edit_job_path(@job) %></div>
<div><%= link_to "Delete", job_path(@job), method: :delete, data: { confirm: "Are you sure??"} %></div>